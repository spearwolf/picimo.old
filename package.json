  "main": "build/picimo.min.js",
  "homepage": "https://github.com/spearwolf/picimo/",
  "bugs": {
    "url": "https://github.com/spearwolf/picimo/issues"
  },
  "scripts": {
    "build:dev": "webpack --progress --colors",
    "build:release": "BUILD_DEV=0 webpack --display-error-details -v",
    "api-docs": "gulp api-docs",
    "watch:src": "webpack-dev-server --inline --progress --colors",
    "watch:api-docs": "watch 'npm run api-docs' api-docs/",
    "release": "run-p build:release api-docs",
    "start": "run-p watch:src watch:api-docs",
    "clean": "gulp clean"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-runtime",
      "transform-decorators",
      "transform-object-assign"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-decorators": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.5",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.3.19",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chalk": "^1.1.1",
    "copy-webpack-plugin": "^1.1.1",
    "del": "^2.2.0",
    "fastclick": "^1.0.6",
    "glob": "^6.0.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-minify-html": "^1.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.5",
    "js-yaml": "^3.5.2",
    "marked": "^0.3.5",
    "npm-run-all": "^2.3.0",
    "through2": "^2.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^0.19.1",
    "watchify": "^3.7.0",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1",
    "yaml-front-matter": "3.2.3"
  },
  "dependencies": {
    "color-js": "^1.0.3",
    "eventize-js": "^0.0.11",
    "gl-matrix": "^2.3.2",
    "lodash": "^4.10.0"
  }
